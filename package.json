{
  "name": "web-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "test": "turbo test",
    "_eslint": "eslint --max-warnings 0",
    "_prettier": "prettier --check",
    "format:check": "pnpm _prettier . && pnpm _eslint .",
    "format": "pnpm _prettier --write . && pnpm _eslint --fix .",
    "check-types": "turbo check-types",
    "check": "pnpm check-types && pnpm format:check",
    "prepare": "husky"
  },
  "devDependencies": {
    "@eslint/js": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "eslint": "catalog:",
    "eslint-config-prettier": "catalog:",
    "eslint-plugin-react-hooks": "catalog:",
    "eslint-plugin-react-refresh": "catalog:",
    "eslint-plugin-storybook": "catalog:",
    "globals": "catalog:",
    "husky": "catalog:",
    "lint-staged": "catalog:",
    "prettier": "catalog:",
    "turbo": "^2.1.2",
    "typescript": "catalog:",
    "typescript-eslint": "catalog:"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@9.9.0"
}
