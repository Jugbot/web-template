{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "env": ["POSTGRES_URL"],
      "inputs": ["$TURBO_DEFAULT$", ".env", ".env.local"],
      "outputs": ["dist/**"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "inputs": ["$TURBO_DEFAULT$", ".env", ".env.local"],
      "persistent": true,
      "interactive": false,
      "cache": false
    },
    "test": {}
  }
}
